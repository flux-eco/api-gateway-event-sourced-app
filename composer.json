{
  "name": "flux-eco/api-gateway-event-sourced-app",
  "description": "Manage requests from outside for an event sourced application",
  "version": "0.0.2",
  "type": "flux-app",
  "keywords": [
    "flux-eco",
    "api-gateway-event-sourced-app",
    "fluxlabs"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "require": {
    "flux-eco/projection": ">=0.0.1",
    "flux-eco/aggregate-root": ">=0.0.1",
    "flux-eco/value-object": ">=0.0.1",
    "flux-eco/ui-transformer": ">=0.0.1",
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "composer-runtime-api": ">=2.1",
    "psr/http-factory": ">=1.0",
    "psr/http-message": ">=1.0",
    "psr/http-server-handler": ">=1.0",
    "psr/http-server-middleware": ">=1.0"
  },
  "require-dev": {
    "composer/composer": ">=2.0",
    "swoole/ide-helper": ">=4.6"
  },
  "autoload": {
    "psr-4": {
      "FluxEco\\ApiGatewayEventSourcedApp\\": [
        "src/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
