{
  "name": "flux-eco/api-gateway-event-sourced-app",
  "description": "Component for flux-capacitor apps developed by fluxlabs",
  "version": "0.0.1",
  "type": "flux-app",
  "keywords": [
    "flux-ecosystem",
    "flux-capacitor",
    "fluxlabs ag"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/projection.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/aggregate-root.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/value-object.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/ui-transformer.git"
    }
  ],
  "require": {
    "flux-eco/projection": "dev-main",
    "flux-eco/aggregate-root": "dev-main",
    "flux-eco/value-object": "dev-main",
    "flux-eco/ui-transformer": "dev-main",
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "composer-runtime-api": ">=2.1",
    "psr/http-factory": ">=1.0",
    "psr/http-message": ">=1.0",
    "psr/http-server-handler": ">=1.0",
    "psr/http-server-middleware": ">=1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.5",
    "composer/composer": ">=2.0",
    "swoole/ide-helper": ">=4.6",
    "symfony/console": ">=5.2"
  },
  "autoload": {
    "psr-4": {
      "FluxEco\\ApiGatewayEventSourcedApp\\": [
        "src/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
